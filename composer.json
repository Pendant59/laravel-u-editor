{
  "name": "shenglin-php/laravel-u-editor",
  "description": "UEditor for laravel5. Support i18n. UEditor is a Rich Text Web Editor From Baidu. Add ali-oss filesystems. Forked by stevenyangecho/laravel-u-editor.",
  "keywords": [
    "laravel",
    "ueditor",
    "laravel-u-editor",
    "laravel5 web text editor",
    "oss",
    "qiniu",
    "shenglin-php"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "shenglin-php",
      "email": "s617790418@126.com"
    }
  ],

  "require": {
    "php": ">=5.4.0",
    "ext-fileinfo": "*",
    "illuminate/support": "5.*",
    "qiniu/php-sdk": "7.*",
    "aliyuncs/oss-sdk-php": "^2.3"
  },
  "autoload": {
    "psr-4": {
      "Shenglin\\UEditor\\": "src/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
